{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "palette": [
        {
          "slug": "headings",
          "color": "#1c1c27",
          "name": "Headings"
        },
        {
          "slug": "text",
          "color": "#313245",
          "name": "Text"
        },
        {
          "slug": "text-light",
          "color": "#73769c",
          "name": "Text Light"
        },
        {
          "slug": "primary",
          "color": "#0e1ecc",
          "name": "Primary"
        },
        {
          "slug": "primary-dark",
          "color": "#04093d",
          "name": "Primary Dark"
        },
        {
          "slug": "secondary",
          "color": "#108e7c",
          "name": "Secondary"
        },
        {
          "slug": "secondary-light",
          "color": "#d9f2ef",
          "name": "Secondary Light"
        },
        {
          "slug": "white",
          "color": "#ffffff",
          "name": "White"
        }
      ],
      "gradients": [
        {
          "slug": "",
          "gradient": "",
          "name": ""
        }
      ]
    },
    "spacing": {
      "units": ["rem", "em", "px", "%", "vh", "vw"]
    },
    "layout": {
      "contentSize": "42.5rem",
      "wideSize": "clamp(48.5rem, 62vw, 96rem)"
    },
    "typography": {
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "Poppins, -apple-system, BlinkMacSystemFont, \"Segoe UI\", \"Ubuntu\", \"DejaVu Sans\", \"Nimbus Sans L\", \"Helvetica Neue\", sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\"",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, \"Segoe UI\", \"Ubuntu\", \"DejaVu Sans\", \"Nimbus Sans L\", \"Helvetica Neue\", sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\"",
          "name": "System Font",
          "slug": "system-font"
        }
      ],
      "fontSizes": [
        {
          "slug": "tiny",
          "size": "clamp(0.89rem, 0.96rem + -0.36vw, 0.70rem)",
          "name": "Tiny"
        },
        {
          "slug": "small",
          "size": "clamp(1.00rem, 1.02rem + -0.12vw, 0.94rem)",
          "name": "Small"
        },
        {
          "slug": "normal",
          "size": "clamp(1.13rem, 1.08rem + 0.24vw, 1.25rem)",
          "name": "Normal"
        },
        {
          "slug": "medium",
          "size": "clamp(1.27rem, 1.11rem + 0.78vw, 1.67rem)",
          "name": "Medium"
        },
        {
          "slug": "large",
          "size": "clamp(1.42rem, 1.11rem + 1.56vw, 2.22rem)",
          "name": "Large"
        },
        {
          "slug": "extra-large",
          "size": "clamp(1.60rem, 1.07rem + 2.65vw, 2.96rem)",
          "name": "Extra Large"
        },
        {
          "slug": "huge",
          "size": "clamp(1.80rem, 0.96rem + 4.19vw, 3.95rem)",
          "name": "Huge"
        },
        {
          "slug": "gigantic",
          "size": "clamp(2.03rem, 0.77rem + 6.31vw, 5.26rem)",
          "name": "Gigantic"
        }
      ]
    },
    "custom": {
      "border-radius": "0",
      "line-height": {
        "body": 1.5,
        "heading": "calc(2px + 2ex + 2px)"
      },
      "spacing": {
        "body": "0",
        "default": "1.5rem",
        "small": "1rem",
        "extra-small": ".5rem",
        "tiny": ".25rem"
      },
      "letter-spacing": {
        "heading": "-0.1rem"
      }
    }
  },
  "styles": {
    "spacing": {
      "blockGap": "var(--wp--custom--spacing--default)",
      "margin": {
        "top": "0px",
        "bottom": "0px"
      }
    },
    "color": {
      "background": "",
      "text": "var(--wp--preset--color--text)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--primary)",
      "lineHeight": "var(--wp--custom--line-height--body)",
      "fontSize": "var(--wp--preset--font-size--normal)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontWeight": "700",
          "letterSpacing": "var(--wp--custom--letter-spacing--heading)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontSize": "var(--wp--preset--font-size--huge)"
        },
        "color": {
          "text": "var(--wp--preset--color--headings)"
        }
      },
      "h2": {
        "typography": {
          "fontWeight": "700",
          "letterSpacing": "var(--wp--custom--letter-spacing--heading)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontSize": "var(--wp--preset--font-size--extra-large)"
        },
        "color": {
          "text": "var(--wp--preset--color--headings)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--text)"
        }
      }
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": "var(--wp--custom--border-radius)"
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--white)"
        },
        "spacing": {
          "padding": {
            "bottom": "var(--wp--custom--spacing--small)",
            "left": "var(--wp--custom--spacing--small)",
            "right": "var(--wp--custom--spacing--small)",
            "top": "var(--wp--custom--spacing--small)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "700"
        }
      },
      "core/heading": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        }
      },
      "core/paragraph": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        }
      },
      "core/post-date": {
        "color": {
          "text": "var(--wp--preset--color--text-light)"
        }
      },
      "core/post-featured-image": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        }
      },
      "core/post-terms": {
        "typography": {
          "lineHeight": "1"
        },
        "color": {
          "background": "var(--wp--preset--color--secondary-light)"
        },
        "spacing": {
          "padding": {
            "bottom": "var(--wp--custom--spacing--tiny)",
            "left": "var(--wp--custom--spacing--tiny)",
            "right": "var(--wp--custom--spacing--tiny)",
            "top": "var(--wp--custom--spacing--tiny)"
          }
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ],
  "customTemplates": [
    {
      "name": "",
      "title": "",
      "postTypes": ["post", "page"]
    }
  ]
}
